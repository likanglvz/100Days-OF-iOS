#import <Foundation/Foundation.h>

int main(int argc, const char * argv[]) {
    int a = 10;
    NSNumber *n1 = [NSNumber numberWithInt:a];
    n1 = @10;
    n1 = @(a);
    int m = n1.intValue;
    
    NSString *s1 = n1.stringValue;
    NSLog(@"m=%d,s1=%@",m,s1);
    return 0;
}
